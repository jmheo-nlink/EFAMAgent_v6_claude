<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="Link.EFAM.Agent.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="Link.EFAM.Agent.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
        </sectionGroup>
    </configSections>

    <!-- 디버깅 및 추적 -->
    <system.diagnostics>
        <switches>
            <add name="traceSwitch" value="0" />
        </switches>
    </system.diagnostics> 

    <!-- log4net 환경 설정 섹션 -->
    <log4net>
        <!-- 사용할 Appender를 정의 -->
        <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
            <file value="log\error_" />
            <appendToFile value="true" />
            <staticLogFileName value="false" />
            <rollingStyle value="Composite" />
            <datePattern value="yyyyMMdd.lo\g" />
            <maxSizeRollBackups value="-1" />
            <maximumFileSize value="1MB" />

            <layout type="log4net.Layout.PatternLayout">
                <conversionPattern value="%date [%thread] %-5level - %message%newline" />
            </layout>
        </appender>

        <!-- 사용할 Logger를 설정 -->
        <root>
            <level value="ERROR" />
            <appender-ref ref="LogFileAppender" />
        </root>
    </log4net>

    <userSettings>
        <Link.EFAM.Agent.Properties.Settings>
            <setting name="IsFirstRun" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="UserId" serializeAs="String">
                <value />
            </setting>
            <setting name="Password" serializeAs="String">
                <value />
            </setting>
            <setting name="AutoRun" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="AutoLogin" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="ServerUrl" serializeAs="String">
                <value>http://124.137.245.171/EFAMServerad</value>
            </setting>
        </Link.EFAM.Agent.Properties.Settings>
    </userSettings>

    <startup>
        
    <supportedRuntime version="v2.0.50727" /></startup>
    <applicationSettings>
        <Link.EFAM.Agent.Properties.Settings>
            <setting name="EFAMUpdater" serializeAs="String">
                <value>Update.exe</value>
            </setting>
            <setting name="IsInSameDomainWithNAS" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="HideFormInSameDomainWithNAS" serializeAs="String">
                <value>False</value>
            </setting>
        </Link.EFAM.Agent.Properties.Settings>
    </applicationSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="LKCorLib" publicKeyToken="59057779c68dfdc9" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.0.4.0" newVersion="1.0.4.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
